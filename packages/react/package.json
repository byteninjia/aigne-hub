{
  "name": "@blocklet/ai-kit",
  "version": "0.0.27",
  "description": "The react.js component library for AI Kit",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-kit#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/es/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/es/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./components/": {
      "import": "./lib/es/components/",
      "require": "./lib/cjs/components/"
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-kit.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "lint-staged",
    "build": "vite build && tsc -p tsconfig.types.json",
    "dev": "npm run storybook & npm run watch",
    "start": "npm run storybook",
    "watch": "vite build --watch",
    "prepublishOnly": "npm run build",
    "preview": "npm run storybook",
    "storybook": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6009",
    "build-storybook": "build-storybook",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@arcblock/eslint-config": "^0.2.3",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.31.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^6.5.15",
    "typescript": "^4.9.4",
    "vite": "^3.2.3",
    "vite-plugin-build": "^0.7.1"
  },
  "dependencies": {
    "@arcblock/ux": "^2.4.65",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.11.10",
    "ahooks": "^3.7.4",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "react-photo-view": "^1.2.3"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  }
}
