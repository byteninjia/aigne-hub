{
  "name": "@blocklet/ai-kit",
  "version": "0.1.28",
  "description": "The react.js component library for AI Kit",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-kit#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./api": {
      "import": "./lib/esm/api/index.js",
      "require": "./lib/cjs/api/index.js"
    },
    "./api/call": {
      "import": "./lib/esm/api/call/index.js",
      "require": "./lib/cjs/api/call/index.js"
    },
    "./api/types": {
      "import": "./lib/esm/api/types/index.js",
      "require": "./lib/cjs/api/types/index.js"
    },
    "./api/*": {
      "import": "./lib/esm/api/*.js",
      "require": "./lib/cjs/api/*.js"
    },
    "./components": {
      "import": "./lib/esm/components/index.js",
      "require": "./lib/cjs/components/index.js"
    },
    "./components/*": {
      "import": "./lib/esm/components/*.js",
      "require": "./lib/cjs/components/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ],
      "api": [
        "./lib/types/api/index.d.ts"
      ],
      "api/*": [
        "./lib/types/api/*.d.ts",
        "./lib/types/api/*/index.d.ts"
      ],
      "components": [
        "./lib/types/components/index.d.ts"
      ],
      "components/*": [
        "./lib/types/components/*.d.ts",
        "./lib/types/components/*/index.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-kit.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "lint-staged",
    "build": "run-p build:*",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:esm": "tsc --module es2022 --outDir lib/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir lib/types",
    "dev": "npm run storybook & npm run watch",
    "start": "npm run storybook",
    "watch": "vite build --watch",
    "prepublishOnly": "npm run build",
    "preview": "npm run storybook",
    "storybook": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6009",
    "build-storybook": "build-storybook",
    "prepare": "npm run build",
    "clean": "rm -rf lib"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@arcblock/did": "^1.18.115",
    "@arcblock/ux": "^2.9.73",
    "@blocklet/logger": "^1.16.26",
    "@blocklet/payment-js": "^1.13.236",
    "@blocklet/sdk": "^1.16.26",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@ocap/mcrypto": "^1.18.115",
    "@types/express": "^4.17.21",
    "@types/express-http-proxy": "^1.6.6",
    "ahooks": "^3.7.11",
    "axios": "^1.6.8",
    "eventsource-parser": "^1.1.2",
    "express": "^4.19.2",
    "express-http-proxy": "^2.0.0",
    "file-saver": "^2.0.5",
    "form-data": "^4.0.0",
    "immer": "^10.1.1",
    "joi": "^17.13.0",
    "json-stable-stringify": "^1.1.1",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "openai": "^4.38.5",
    "react-markdown": "^8.0.7",
    "react-photo-view": "^1.2.4",
    "react-use": "^17.5.0",
    "recoil": "^0.7.7",
    "sequelize": "^6.37.3",
    "ufo": "^1.5.3",
    "yaml": "^2.4.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/express": "^4.17.21",
    "@types/file-saver": "^2.0.7",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^6.5.16",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-build": "^0.7.1"
  }
}
