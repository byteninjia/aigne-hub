{
  "name": "@blocklet/ai-kit",
  "version": "0.0.94",
  "description": "The react.js component library for AI Kit",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-kit#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/es/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/es/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./api": {
      "import": "./lib/es/api/index.js",
      "require": "./lib/cjs/api/index.js"
    },
    "./api/": {
      "import": "./lib/es/api/",
      "require": "./lib/cjs/api/"
    },
    "./components": {
      "import": "./lib/es/components/index.js",
      "require": "./lib/cjs/components/index.js"
    },
    "./components/": {
      "import": "./lib/es/components/",
      "require": "./lib/cjs/components/"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ],
      "api": [
        "./lib/types/api/index.d.ts"
      ],
      "api/*": [
        "./lib/types/api/*.d.ts",
        "./lib/types/api/*/index.d.ts"
      ],
      "components": [
        "./lib/types/components/index.d.ts"
      ],
      "components/*": [
        "./lib/types/components/*.d.ts",
        "./lib/types/components/*/index.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-kit.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "lint-staged",
    "build": "vite build && tsc -p tsconfig.types.json",
    "dev": "npm run storybook & npm run watch",
    "start": "npm run storybook",
    "watch": "vite build --watch",
    "prepublishOnly": "npm run build",
    "preview": "npm run storybook",
    "storybook": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6009",
    "build-storybook": "build-storybook",
    "prepare": "npm run build",
    "update:deps": "npx taze -n '/arcblock|ocap|abtnode|blocklet|did-connect|did-comment|nedb/' -w && npm run deduplicate && yarn",
    "deduplicate": "npx yarn-deduplicate ../../yarn.lock"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@arcblock/ux": "^2.8.21",
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.11.10",
    "ahooks": "^3.7.4",
    "axios": "^1.3.4",
    "file-saver": "^2.0.5",
    "immer": "^9.0.19",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.1",
    "openai": "^4.17.5",
    "react-markdown": "^8.0.6",
    "react-photo-view": "^1.2.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^6.5.15",
    "typescript": "^4.9.4",
    "vite": "^4.1.4",
    "vite-plugin-build": "^0.7.1"
  }
}
