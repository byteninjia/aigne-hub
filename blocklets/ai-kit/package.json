{
  "name": "ai-kit",
  "version": "0.0.99",
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "scripts": {
    "dev": "blocklet dev",
    "eject": "vite eject",
    "lint": "tsc --noEmit && eslint src api/src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=development nodemon api/dev.ts -w api",
    "clean": "node scripts/build-clean.js",
    "build": "run-s clean bundle:client bundle:api",
    "bundle": "run-s clean bundle:*",
    "bundle:client": "vite build",
    "bundle:api": "tsc -p tsconfig.api.json && tsc-alias -p tsconfig.api.json",
    "bundle:blocklet": "blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "update:deps": "npx taze -n '/arcblock|ocap|abtnode|blocklet|did-connect|did-comment|nedb/' -w && npm run deduplicate && yarn",
    "deduplicate": "npx yarn-deduplicate ../../yarn.lock"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcblock/did": "^1.18.106",
    "@arcblock/did-auth": "^1.18.105",
    "@arcblock/did-auth-storage-nedb": "^1.7.1",
    "@arcblock/did-connect": "^2.8.21",
    "@arcblock/ux": "^2.8.21",
    "@blocklet/ai-kit": "^0.0.99",
    "@blocklet/sdk": "^1.16.19",
    "@blocklet/ui-react": "^2.8.21",
    "@did-pay/client": "^1.13.72",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@google/generative-ai": "^0.1.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "@ocap/client": "^1.18.105",
    "@ocap/mcrypto": "^1.18.105",
    "@ocap/wallet": "^1.18.105",
    "ahooks": "^3.7.4",
    "axios": "^1.6.2",
    "bignumber.js": "^9.1.2",
    "compose-middleware": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-history-api-fallback": "^2.2.1",
    "express-http-proxy": "^2.0.0",
    "gpt-tokens": "^1.1.2",
    "https-proxy-agent": "^7.0.2",
    "immer": "^9.0.19",
    "joi": "^17.7.1",
    "js-tiktoken": "^1.0.8",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "openai": "^4.17.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "sequelize": "^6.32.1",
    "snowflake-uuid": "^1.0.0",
    "sqlite3": "^5.1.6",
    "ufo": "^1.3.2",
    "umzug": "^3.3.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.14",
    "@types/express-http-proxy": "^1.6.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^2.2.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^4.9.4",
    "vite": "^4.1.4",
    "vite-plugin-blocklet": "^0.6.16",
    "vite-plugin-svgr": "^2.2.2",
    "vite-tsconfig-paths": "^4.2.2",
    "zx": "^7.1.1"
  }
}
